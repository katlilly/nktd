#JSGF V1.0;

@author Daniel Davidson

grammar grammars.tetrisCommand;

<direction> = left | right;
<multiplier> = one | two | three | four | five | six | seven | eight | nine;

@example left five
<movement> = <direction> <multiplier>;

@example flip three
<flip> = flip (one | two | three);

<command> = <movement> | <flip>;